{
  "app": "npx ts-node bin/deepseek-r1-sagemaker.ts",
  "context": {
    "containerType": "tgi",
    "instanceType": "ml.g5.2xlarge",
    "modelName": "Qwen-7B"
  },
  "postDeploy": [
    "export SAGEMAKER_ENDPOINT_NAME=$(cdk output SageMakerEndpointName)",
    "export AWS_REGION=$(cdk output AWSRegion)",
    "pip install -r test/requirements.txt",
    "python test/sagemaker-endpoint-test.py"
  ]
}
